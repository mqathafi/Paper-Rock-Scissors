train_generator = train_datagen.flow_from_directory(train_dir,
                                                    target_size=(200, 200),
                                                    batch_size=4,
                                                    class_mode='categorical')

                                                    #subset='training',)

validation_generator = train_datagen.flow_from_directory(validation_dir,
                                                    target_size=(200, 200),
                                                    batch_size=4,
                                                    class_mode='categorical')
                                                    #subset='validation',)
